{
    "name": "share",
    "displayName": "Share",
    "description": "Cooperative editing in vscode",
    "version": "0.3.0",
    "publisher": "lyuboraykov",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.shareEditor",
        "onCommand:extension.connectToEditor"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.shareEditor",
                "title": "Share Editor"
            },
            {
                "command": "extension.connectToEditor",
                "title": "Connect to Editor"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    },
    "dependencies": {
        "firebase": "^3.6.4"
    }
}